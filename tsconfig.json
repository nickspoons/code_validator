{
   "compileOnSave": false,
   "compilerOptions": {
      "plugins": [
         {
           "name": "typescript-tslint-plugin"
         }
       ],
      "baseUrl": "./",
      "declaration": false,
      "downlevelIteration": true,
      "esModuleInterop": true,
      "experimentalDecorators": true,
      "forceConsistentCasingInFileNames": true,
      "importHelpers": true,
      "isolatedModules": true,
      "lib": ["es2018", "dom"],
      "module": "es2020",
      "moduleResolution": "node",
      "outDir": "./dist/out-tsc",
      "sourceMap": true,
      "target": "es2015",
      "typeRoots": [
         "node_modules/@types"
      ]
   }
}
